{
  "projectType": "library",
  "root": "libs/shell-web",
  "sourceRoot": "libs/shell-web/src",
  "prefix": "mx-onboarding-workspace",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shell-web"],
      "options": {
        "jestConfig": "libs/shell-web/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shell-web/src/**/*.ts",
          "libs/shell-web/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
