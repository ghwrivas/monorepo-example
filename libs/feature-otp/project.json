{
  "projectType": "library",
  "root": "libs/feature-otp",
  "sourceRoot": "libs/feature-otp/src",
  "prefix": "mx-onboarding-workspace",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/feature-otp"],
      "options": {
        "jestConfig": "libs/feature-otp/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/feature-otp/src/**/*.ts",
          "libs/feature-otp/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
